   "source": [
    "import pyttsx3\n",
    "sum=0\n",
    "fp=open(\"E:\\\\example1.txt\",\"r\")\n",
    "num = fp.read()\n",
    "for i in num.split(\",\"):\n",
    "    sum=sum+int(i)\n",
    "fp.close()\n",
    "engine=pyttsx3.init()\n",
    "print(num)\n",
    "engine.say(sum)\n",
    "\n",
    "engine.runAndWait()"
   ]
